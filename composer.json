{
    "name": "scaleplan/php-mattermost-driver",
    "description": "The Php Driver to interact with the Mattermost Web Service API",
    "keywords": ["mattermost", "chat"],
    "homepage": "https://github.com/gnello/php-mattermost-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Agnello",
            "email": "luca@gnello.com"
        },
        {
            "name": "Aleksandr Avtomonov",
            "email": "admin@qooiz.me"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-json": "*",
        "psr/http-message" : "^1.0",
        "pimple/pimple"    : "~3.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/var-dumper": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Scaleplan\\Mattermost\\": "src/"
        }
    }
}
